// TODO: change prizes data

-
  var prises_data = [
    {
      imgSrc:'assets/logos/VKLogo.svg',
      width:'120px',
      height:'120px',
      alt:'Логотип VK',
      description: 'Консультация от VK'
    },{
      imgSrc:'assets/prises/100.svg',
      width:'150px',
      height:'150px',
      alt:'Деньги',
      description:'150 000 рублей на каждый трек'
    }, {
      imgSrc:'assets/logos/MIREA_logo2.webp',
      width:'150px',
      height:'170px',
      alt:'Логотип РТУ МИРЭА',
      description:'Призы от партнёров'
    }] 
 
mixin prise-block(data)
  .prise-block
    img.logo(src=data.imgSrc width=data.width height=data.height alt=data.alt)
    p.description
      = data.description

section.prises#prises-section
  h2.prises__header Призы

  .prises__content
    each prise in prises_data
      +prise-block(prise)
